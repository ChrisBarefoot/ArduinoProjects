int gcount = 1;

void setup() {
  strip.begin();
  strip.setBrightness(80);
  strip.show(); // Initialize all pixels to 'off'
}

void loop() {
  showImage(gcount++);
  if(gcount>3)
  {
    gcount = 1;
  }
  delay(140);
}

void setPixel(int i, byte r, byte g, byte b)
{
  int row = i / 16;
  if(row % 2 == 0)
  {
    strip.setPixelColor(i, r, g, b);
  }
  else
  {
    strip.setPixelColor( (((row+1)*16)-1) - (i - (row)*16), r, g, b);
  }
}

// Fill the dots one after the other with a color
void showImage(int count)
{
  for(uint16_t i = 0; i < strip.numPixels(); i++)
  {
    if(count == 1)
    {
      byte r = pgm_read_byte_near(rs1 + i);
      byte g = pgm_read_byte_near(gs1 + i);
      byte b = pgm_read_byte_near(bs1 + i);
      setPixel(i, r, g, b);
    }
    else if(count == 2)
    {
      byte r = pgm_read_byte_near(rs2 + i);
      byte g = pgm_read_byte_near(gs2 + i);
      byte b = pgm_read_byte_near(bs2 + i);
      setPixel(i, r, g, b);
    }
    else if(count == 3)
    {
      byte r = pgm_read_byte_near(rs3 + i);
      byte g = pgm_read_byte_near(gs3 + i);
      byte b = pgm_read_byte_near(bs3 + i);
      setPixel(i, r, g, b);
    }
   
  }
  strip.show();
}



